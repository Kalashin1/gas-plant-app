<div class="container-fluid">
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-header">
          <h3 class="card-title text-primary">View Expenses Made</h3>
        </div>
        <!-- /.card-header -->
        <div class="card-body">
          <table id="customer-table" class="table table-striped table-hover">
            <thead>
              <tr>
                <th>Item</th>
                <th>Price</th>
                <th>Quantity</th>
                <th>Seller</th>
                <th>Seller Number</th>
                <th>Bought By</th>
                <th>Date</th>
              </tr>
            </thead>
            <tbody id="expenses-table"></tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-6">
      <div class="btn btn-outline-success" id="load-more">
        Load More
      </div>
    </div>
    <div class="col-6 justify-content-end d-flex align-content-end">
      <div class="btn btn-outline-success" id="load-all">
        Load All
      </div>
    </div>
  </div>
</div>
